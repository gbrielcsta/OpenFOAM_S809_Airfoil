FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

kInlet          0.032;

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $kInlet;

boundaryField
{
	airfoil
	{
		type            kqRWallFunction;
        value           uniform $kInlet;
	}
	
	outlet
    {
		type            fixedValue;
        value           uniform $kInlet;
    }
	
    inlet
    {
        type            fixedValue;
        value           uniform $kInlet;
    }
	
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
