FoamFile
{
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

omegaInlet      4.08;

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $omegaInlet;

boundaryField
{
	airfoil
	{
        type            omegaWallFunction;
        value           uniform $omegaInlet;
	}
	
	outlet
    {
		type            fixedValue;
        value           uniform $omegaInlet;
    }
	
    inlet
    {
        type            fixedValue;
        value           uniform $omegaInlet;
    }

    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
